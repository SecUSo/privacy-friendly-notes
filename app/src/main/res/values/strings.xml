    <string name="help_perm_record_audio_title">RECORD AUDIO</string>
    <string name="help_perm_record_audio_sum">is needed to record audio notes; if this is not granted, audio notes are unavailable.</string>
    <string name="help_perm_write_external_title">WRITE EXTERNAL STORAGE</string>
    <string name="help_perm_write_external_sum">is needed to save notes to the internal memory; if this is not granted, notes cannot be saved externally.</string>
